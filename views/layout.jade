doctype 5
html
    head
        title= title
        link(rel='stylesheet', href='/stylesheets/bootstrap.css')
        link(rel='stylesheet', href='/stylesheets/style.css')
        block styles
        script(src='//ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js')
        script(src='/javascripts/big.js')
        script(src='/javascripts/soundmanager2.js')
        script(src='/javascripts/zeroTimeout.js')
        script(src='/javascripts/main.js')
        block scripts
    body
    div.container.heading
        div.row
            div.span1
            div.span10
                block heading
                    h1= title
                    p Welcome to #{title}
            div.span1
    div.container.content
        block content